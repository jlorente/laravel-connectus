{
    "name": "jlorente/laravel-connectus",
    "description": "Laravel integration for the Jlorente Connectus php sdk.",
    "keywords": [
        "php",
        "jlorente",
        "laravel",
        "connectus"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "JosÃ© Lorente",
            "email": "jose.lorente.martin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": ">=5.6",
        "jlorente/connectus-php-sdk": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Jlorente\\Laravel\\Connectus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jlorente\\Laravel\\Connectus\\ConnectusServiceProvider"
            ],
            "aliases": {
                "Connectus": "Jlorente\\Laravel\\Connectus\\Facades\\Connectus"
            }
        }
    },
    "minimum-stability": "stable"
}
